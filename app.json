{
  "expo": {
    "name": "ElehaNafshi",
    "slug": "eleha-nafshi",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/iconApp.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mycompany.elehanafchi",
      "buildNumber": "4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Cette application n'utilise pas la caméra.",
        "NSPhotoLibraryUsageDescription": "Cette application n'accède pas à vos photos.",
        "NSLocationWhenInUseUsageDescription": "Cette application utilise votre localisation pour afficher les Kivrei Tsadikim les plus proches.",
        "NSMicrophoneUsageDescription": "Cette application n'utilise pas le microphone."
      }
    },
    "android": {
      "package": "com.mycompany.elehanafchi",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iconApp.png",
        "backgroundColor": "#8B5CF6"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c01ddb79-6c37-4cfd-9a4c-dfa1baeedb8d"
      }
    }
  }
}
